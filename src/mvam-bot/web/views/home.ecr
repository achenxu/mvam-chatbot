<div class="starter-template">
  <h1>WFP mVAM chatbot</h1>
  <p class="lead">Telegram chatbot for answering prices queries and running food security surveys</p>
</div>

<h1>Users</h1>

<div class="list-group">
<% users.each do |user| %>
  <a href="/users/<%= user.id %>" class="list-group-item"><%= user.full_name %></a>
<% end %>
</div>
